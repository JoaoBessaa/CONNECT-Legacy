<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>DocQuery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docquery.nhin.DocQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocQuery</servlet-name>
        <url-pattern>/NhinService/RespondingGateway_Query_Service/DocQuery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DocRetrieve</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.DocRetrieve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocRetrieve</servlet-name>
        <url-pattern>/NhinService/RespondingGateway_Retrieve_Service/DocRetrieve</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HiemNotify</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.notify.HiemNotify</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemNotify</servlet-name>
        <url-pattern>/NhinService/NotificationConsumerService/HiemNotify</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HiemSubscription</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.subscription.HiemSubscription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemSubscription</servlet-name>
        <url-pattern>/NhinService/NotificationProducerService/HiemSubscription</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.hiem.nhin.unsubscribe.HiemUnsubscribe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <url-pattern>/NhinService/SubscriptionManagerService/HiemUnsubscribe</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinPatientDiscovery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.patientdiscovery.nhin.NhinPatientDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinPatientDiscovery</servlet-name>
        <url-pattern>/NhinService/NhinPatientDiscovery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinPatientDiscoveryAsyncReq</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.patientdiscovery.nhin.deferred.request.NhinPatientDiscoveryAsyncReq</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinPatientDiscoveryAsyncReq</servlet-name>
        <url-pattern>/NhinService/NhinPatientDiscoveryAsyncReq</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinPatientDiscoveryAsyncResp</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.patientdiscovery.nhin.deferred.response.NhinPatientDiscoveryAsyncResp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinPatientDiscoveryAsyncResp</servlet-name>
        <url-pattern>/NhinService/NhinPatientDiscoveryAsyncResp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubjectDiscovery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.subjectdiscovery.nhin.SubjectDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectDiscovery</servlet-name>
        <url-pattern>/NhinService/NhinSubjectDiscovery</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDR</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.NhinXDR</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDR</servlet-name>
        <url-pattern>/DocumentRepositoryXDR_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDRRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.deferred.request.NhinXDRRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRRequest</servlet-name>
        <url-pattern>/NhinService/XDRRequest_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinXDRResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docsubmission.nhin.deferred.response.NhinXDRResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRResponse</servlet-name>
        <url-pattern>/NhinService/XDRResponse_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AuditQuery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.auditquery.nhin.AuditQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuditQuery</servlet-name>
        <url-pattern>/NhinService/findAuditEvents/AuditQuery</url-pattern>
    </servlet-mapping>
    <!-- BEGIN: Document Retrieve Deferred Services -->
    <servlet>
        <servlet-name>NhinDocRetrieveDeferredRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.deferred.request.NhinDocRetrieveDeferredRequest</servlet-class>
    </servlet>
