        <servlet-name>NhinDocRetrieveDeferredRequest</servlet-name>
        <url-pattern>/NhinService/RespondingGatewayDeferredRequest_Retrieve_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinDocRetrieveDeferredResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.nhin.deferred.response.NhinDocRetrieveDeferredResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinDocRetrieveDeferredResponse</servlet-name>
        <url-pattern>/NhinService/RespondingGatewayDeferredResponse_Retrieve_Service</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinDocQueryDeferredRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docquery.nhin.deferred.request.NhinDocQueryDeferredRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinDocQueryDeferredRequest</servlet-name>
        <url-pattern>/NhinService/DocQueryDeferredRequestService</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NhinDocQueryDeferredResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docquery.nhin.deferred.response.NhinDocQueryDeferredResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinDocQueryDeferredResponse</servlet-name>
        <url-pattern>/NhinService/DocQueryDeferredResponseService</url-pattern>
    </servlet-mapping>
    <!-- END: Document Retrieve Deferred Services -->
    <servlet>
        <servlet-name>NhinAdministrativeDistribution</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.admindistribution.nhin.NhinAdministrativeDistribution</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinAdministrativeDistribution</servlet-name>
        <url-pattern>/NhinService/NhinAdminDist</url-pattern>
    </servlet-mapping>
    <security-constraint>
        <web-resource-collection>
            <web-resource-name>NhinAdministrativeDistribution</web-resource-name>
            <url-pattern>/NhinService/NhinAdminDist</url-pattern>
        </web-resource-collection>

        <web-resource-collection>
            <web-resource-name>DocQuery</web-resource-name>
            <url-pattern>/NhinService/RespondingGateway_Query_Service/DocQuery</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>DocRetrieve</web-resource-name>
            <url-pattern>/NhinService/RespondingGateway_Retrieve_Service/DocRetrieve</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemNotify</web-resource-name>
            <url-pattern>/NhinService/NotificationConsumerService/HiemNotify</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemSubscription</web-resource-name>
            <url-pattern>/NhinService/NotificationProducerService/HiemSubscription</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemUnsubscribe</web-resource-name>
            <url-pattern>/NhinService/SubscriptionManagerService/HiemUnsubscribe</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinPatientDiscovery</web-resource-name>
            <url-pattern>/NhinService/NhinPatientDiscovery</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinPatientDiscoveryAsyncReq</web-resource-name>
            <url-pattern>/NhinService/NhinPatientDiscoveryAsyncReq</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinPatientDiscoveryAsyncResp</web-resource-name>
            <url-pattern>/NhinService/NhinPatientDiscoveryAsyncResp</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>SubjectDiscovery</web-resource-name>
            <url-pattern>/NhinService/NhinSubjectDiscovery</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDR</web-resource-name>
            <url-pattern>/DocumentRepositoryXDR_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRRequest</web-resource-name>
            <url-pattern>/NhinService/XDRRequest_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRResponse</web-resource-name>
            <url-pattern>/NhinService/XDRResponse_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>AuditQuery</web-resource-name>
            <url-pattern>/NhinService/findAuditEvents/AuditQuery</url-pattern>
        </web-resource-collection>
        <!-- BEGIN Document Retrieve Deferred Service -->
        <web-resource-collection>
            <web-resource-name>NhinDocRetrieveDeferredRequest</web-resource-name>
            <url-pattern>/NhinService/RespondingGatewayDeferredRequest_Retrieve_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinDocRetrieveDeferredResponse</web-resource-name>
            <url-pattern>/NhinService/RespondingGatewayDeferredResponse_Retrieve_Service</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinDocQueryDeferredRequest</web-resource-name>
            <url-pattern>/NhinService/DocQueryDeferredRequestService</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinDocQueryDeferredResponse</web-resource-name>
            <url-pattern>/NhinService/DocQueryDeferredResponseService</url-pattern>
        </web-resource-collection>
        <!-- END Document Retrieve Deferred Service -->
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>
    <login-config>
        <auth-method>CLIENT-CERT</auth-method>
        <realm-name>certificate</realm-name>
    </login-config>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
