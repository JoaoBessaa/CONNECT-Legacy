<hibernate-mapping package="gov.hhs.fha.nhinc.patientcorrelation.model">
  <class name="CorrelatedIdentifiers" table="correlatedidentifiers">
    <id name="correlationId" column="correlationid" type="long">
        <generator class="increment" />
    </id>
    <property name="patientAssigningAuthorityId" column="PatientAssigningAuthorityId" type="string" length="45" not-null="true" />
    <property name="patientId" column="PatientId" type="string" length="45" not-null="true" />
    <property name="correlatedPatientAssigningAuthorityId" column="CorrelatedPatientAssignAuthId" type="string" length="45" not-null="true" />
    <property name="correlatedPatientId" column="CorrelatedPatientId" type="string" length="45" not-null="true" />
    <property name="correlationExpirationDate" column="CorrelationExpirationDate" type="timestamp" length="45" not-null="false" />
  </class>
  
</hibernate-mapping>
