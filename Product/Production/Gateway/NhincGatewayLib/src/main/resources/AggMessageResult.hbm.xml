<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.hhs.fha.nhinc.gateway.aggregator.model">
  <class name="AggMessageResult" table="agg_message_results">
	<id name="messageId" column="MessageId" type="string" length="32">
        	<generator class="uuid" />
	</id>
    <many-to-one name="aggTransaction" 
                 column="TransactionId" 
                 class="gov.hhs.fha.nhinc.gateway.aggregator.model.AggTransaction" 
                 not-null="true"
                 lazy="false" />
  	<property name="messageKey" column="MessageKey" type="string" length="1000" not-null="true" />
  	<property name="messageOutTime" column="MessageOutTime" type="timestamp" not-null="false" />
  	<property name="responseReceivedTime" column="ResponseReceivedTime" type="timestamp" not-null="false" />
  	<property name="responseMessageType" column="ResponseMessageType" type="string" length="100" not-null="false" />
  	<property name="responseMessageAsBytes" column="responseMessage" type="binary" not-null="false" />
  </class>
</hibernate-mapping>
