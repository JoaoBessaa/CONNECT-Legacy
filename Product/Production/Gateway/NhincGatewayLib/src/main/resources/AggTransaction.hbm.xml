<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.hhs.fha.nhinc.gateway.aggregator.model">
  <class name="AggTransaction" table="agg_transaction">
	<id name="transactionId" column="TransactionId" type="string" length="32">
        	<generator class="uuid" />
	</id>
  	<property name="serviceType" column="ServiceType" type="string" length="64" not-null="true" />
  	<property name="transactionStartTime" column="TransactionStartTime" type="timestamp" not-null="false" />
    <set name="aggMessageResults" inverse="true" lazy="false" cascade="all-delete-orphan">
        <key column="transactionId"/>
        <one-to-many class="gov.hhs.fha.nhinc.gateway.aggregator.model.AggMessageResult"/>            
    </set>
    
  </class>
</hibernate-mapping>