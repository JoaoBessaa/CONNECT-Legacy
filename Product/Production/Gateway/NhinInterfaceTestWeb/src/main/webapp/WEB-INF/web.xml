<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>AdapterMpiSecured</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.adaptermpi.AdapterMpiSecured</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdapterMpiSecured</servlet-name>
        <url-pattern>/AdapterMpiSecuredTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AuditQuery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.auditquery.AuditQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuditQuery</servlet-name>
        <url-pattern>/AuditQueryTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DocQuery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docquery.DocQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocQuery</servlet-name>
        <url-pattern>/DocQueryTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DocRetrieve</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.docretrieve.DocRetrieve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DocRetrieve</servlet-name>
        <url-pattern>/DocRetrieveTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HiemNotify</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.notify.HiemNotify</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemNotify</servlet-name>
        <url-pattern>/HiemNotifyTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NhinPatientDiscovery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.patientdiscovery.NhinPatientDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinPatientDiscovery</servlet-name>
        <url-pattern>/NhinPatientDiscoveryTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SubjectDiscovery</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.subjectdiscovery.SubjectDiscovery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectDiscovery</servlet-name>
        <url-pattern>/SubjectDiscoveryTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HiemSubscription</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.subscription.HiemSubscription</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemSubscription</servlet-name>
        <url-pattern>/HiemSubscriptionTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.unsubscribe.HiemUnsubscribe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HiemUnsubscribe</servlet-name>
        <url-pattern>/HiemUnsubscribeTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NhinXDRRequest</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.xdr.async.request.NhinXDRRequest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRRequest</servlet-name>
        <url-pattern>/NhinService/XDRRequestServiceTestHelper</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NhinXDRResponse</servlet-name>
        <servlet-class>gov.hhs.fha.nhinc.xdr.async.response.NhinXDRResponse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NhinXDRResponse</servlet-name>
        <url-pattern>/NhinService/XDRResponseServiceTestHelper</url-pattern>
    </servlet-mapping>

    <security-constraint>
        <web-resource-collection>
            <web-resource-name>AdapterMpiSecured</web-resource-name>
            <url-pattern>/AdapterMpiSecuredTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>AuditQuery</web-resource-name>
            <url-pattern>/AuditQueryTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>DocQuery</web-resource-name>
            <url-pattern>/DocQueryTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>DocRetrieve</web-resource-name>
            <url-pattern>/DocRetrieveTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemNotify</web-resource-name>
            <url-pattern>/HiemNotifyTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinPatientDiscovery</web-resource-name>
            <url-pattern>/NhinPatientDiscoveryTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>SubjectDiscovery</web-resource-name>
            <url-pattern>/SubjectDiscoveryTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemSubscription</web-resource-name>
            <url-pattern>/HiemSubscriptionTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>HiemUnsubscribe</web-resource-name>
            <url-pattern>/HiemUnsubscribeTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRRequest</web-resource-name>
            <url-pattern>/NhinService/XDRRequestServiceTestHelper</url-pattern>
        </web-resource-collection>
        <web-resource-collection>
            <web-resource-name>NhinXDRResponse</web-resource-name>
            <url-pattern>/NhinService/XDRResponseServiceTestHelper</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
    </security-constraint>

    <login-config>
        <auth-method>CLIENT-CERT</auth-method>
        <realm-name>certificate</realm-name>
    </login-config>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
