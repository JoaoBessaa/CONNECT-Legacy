<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:webproject2="http://www.netbeans.org/ns/web-project/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-service-AuditRepository" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/AuditRepository"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/AuditRepository" destdir="${build.generated.dir}/jax-wsCache/service/AuditRepository" wsdl="${basedir}/${conf-dir}xml-resources/web-services/AuditRepository/wsdl/NhincComponentAuditRepositorySecured.wsdl" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-services/AuditRepository/wsdl/NhincComponentAuditRepositorySecured.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/AuditRepository"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/AuditRepository">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <copy todir="${build.web.dir}/WEB-INF/wsdl/AuditRepository">
            <fileset dir="${basedir}/${conf-dir}xml-resources/web-services/AuditRepository/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-AuditRepository" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhinccomponentauditrepository"/>
        <delete dir="${build.generated.dir}/jax-wsCache/service/AuditRepository"/>
    </target>
    <target name="wsimport-service-PatientCorrelationServiceSecured" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured" destdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured" wsdl="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationServiceSecured/wsdl/NhincComponentPatientCorrelationSecured.wsdl" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationServiceSecured/wsdl/NhincComponentPatientCorrelationSecured.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <copy todir="${build.web.dir}/WEB-INF/wsdl/PatientCorrelationServiceSecured">
            <fileset dir="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationServiceSecured/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PatientCorrelationServiceSecured" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/nhinccomponentpatientcorrelation"/>
        <delete dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationServiceSecured"/>
    </target>
    <target name="wsimport-service-PatientCorrelationFacadeDteService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService" destdir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService" wsdl="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationFacadeDteService/wsdl/NhincComponentPatientCorrelationFacadeDte.wsdl" catalog="catalog.xml" extension="true" verbose="true" xnocompile="true">
            <depends file="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationFacadeDteService/wsdl/NhincComponentPatientCorrelationFacadeDte.wsdl"/>
            <produces dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
        <copy todir="${build.web.dir}/WEB-INF/wsdl/PatientCorrelationFacadeDteService">
            <fileset dir="${basedir}/${conf-dir}xml-resources/web-services/PatientCorrelationFacadeDteService/wsdl/"/>
        </copy>
    </target>
    <target name="wsimport-service-clean-PatientCorrelationFacadeDteService" depends="-init-project">
        <delete dir="${build.generated.sources.dir}/jax-ws/gov/hhs/fha/nhinc/componentpatientcorrelationfacadedte"/>
        <delete dir="${build.generated.dir}/jax-wsCache/service/PatientCorrelationFacadeDteService"/>
    </target>
    <target name="wsimport-service-generate" depends="wsimport-service-AuditRepository, wsimport-service-PatientCorrelationServiceSecured, wsimport-service-PatientCorrelationFacadeDteService"/>
</project>
