<!-- in the project's Project Properties dialog box.-->
<project name="NHINAdapterServiceEJB" default="default" basedir="." xmlns:ejbjarproject="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
  <description>Builds, tests, and runs the project NHINAdapterServiceEJB.</description>
  <import file="nbproject/build-impl.xml"/>

  <property name="deploy.type" value="ejb" />
  <property name="deploy.isDeployable" value="false"/>

  <import file="../../../../build.lib.xml" />

  <target name="build">
    <property name="target-to-call" value="dist" />
    <antcall target="call-target-in-isolation-if-out-of-date" />
  </target>
  <target name="clean-build" depends="clean,build" />
  <target name="clean-test" depends="clean-build,test" />
  <target name="build-test" depends="build,test" />

  <target name="wsimport-client-BOSServiceEndpointProvider" depends="wsimport-init">
    <property name="targetLocation-BOSServiceEndpointProvider" location="${meta.inf}/wsdl/BOSServiceEndpointProvider"/>
    <property name="sourceLocation-BOSServiceEndpointProvider" location="${meta.inf}/xml-resources/web-service-references/BOSServiceEndpointProvider/wsdl/"/>
    <copy todir="${targetLocation-BOSServiceEndpointProvider}">
      <fileset dir="${sourceLocation-BOSServiceEndpointProvider}"/>
    </copy>
    <property name="targetLocation-BOSServiceEndpointProvider-schemas" location="${meta.inf}/wsdl/schemas"/>
    <property name="sourceLocation-BOSServiceEndpointProvider-schemas" location="${meta.inf}/xml-resources/web-service-references/BOSServiceEndpointProvider/schemas/"/>
    <copy todir="${targetLocation-BOSServiceEndpointProvider-schemas}">
      <fileset dir="${sourceLocation-BOSServiceEndpointProvider-schemas}"/>
    </copy>
  </target>

  <target name="wsimport-client-DocumentManager" depends="wsimport-init">
    <property name="targetLocation-DocumentManager" location="${meta.inf}/wsdl/DocumentManager"/>
    <property name="sourceLocation-DocumentManager" location="${meta.inf}/xml-resources/web-service-references/DocumentManager/wsdl/"/>
    <copy todir="${targetLocation-DocumentManager}">
      <fileset dir="${sourceLocation-DocumentManager}"/>
    </copy>
    <property name="targetLocation-DocumentManager-schemas" location="${meta.inf}/wsdl/schemas"/>
    <property name="sourceLocation-DocumentManager-schemas" location="${meta.inf}/xml-resources/web-service-references/DocumentManager/schemas/"/>
    <copy todir="${targetLocation-DocumentManager-schemas}">
      <fileset dir="${sourceLocation-DocumentManager-schemas}"/>
    </copy>
  </target>

  <target name="wsimport-client-DocumentAssembly" depends="wsimport-init">
    <property name="targetLocation-DocumentAssembly" location="${meta.inf}/wsdl/DocumentAssembly"/>
    <property name="sourceLocation-DocumentAssembly" location="${meta.inf}/xml-resources/web-service-references/DocumentAssembly/wsdl/"/>
    <copy todir="${targetLocation-DocumentAssembly}">
      <fileset dir="${sourceLocation-DocumentAssembly}"/>
    </copy>
    <property name="targetLocation-DocumentAssembly-schemas" location="${meta.inf}/wsdl/schemas"/>
    <property name="sourceLocation-DocumentAssembly-schemas" location="${meta.inf}/xml-resources/web-service-references/DocumentAssembly/schemas/"/>
    <copy todir="${targetLocation-DocumentAssembly-schemas}">
      <fileset dir="${sourceLocation-DocumentAssembly-schemas}"/>
    </copy>
  </target>

  <target name="wsimport-service-AdapterDocQuerySecured" depends="wsimport-init">
    <property name="targetLocation-AdapterDocQuerySecured" location="${meta.inf}/wsdl/AdapterDocQuerySecured"/>
    <property name="sourceLocation-AdapterDocQuerySecured" location="${meta.inf}/xml-resources/web-services/AdapterDocQuerySecured/wsdl/"/>
    <copy todir="${targetLocation-AdapterDocQuerySecured}">
      <fileset dir="${sourceLocation-AdapterDocQuerySecured}"/>
    </copy>
    <property name="targetLocation-AdapterDocQuerySecuredSchemas" location="${meta.inf}/wsdl/schemas/"/>
    <property name="sourceLocation-AdapterDocQuerySecuredSchemas" location="${meta.inf}/xml-resources/web-services/AdapterDocQuerySecured/schemas/"/>
    <copy todir="${targetLocation-AdapterDocQuerySecuredSchemas}">
      <fileset dir="${sourceLocation-AdapterDocQuerySecuredSchemas}" />
    </copy>
  </target>

  <target name="wsimport-service-AdapterDocRetrieveSecured" depends="wsimport-init">
    <property name="targetLocation-AdapterDocRetrieveSecured" location="${meta.inf}/wsdl/AdapterDocRetrieveSecured"/>
    <property name="sourceLocation-AdapterDocRetrieveSecured" location="${meta.inf}/xml-resources/web-services/AdapterDocRetrieveSecured/wsdl/"/>
    <copy todir="${targetLocation-AdapterDocRetrieveSecured}">
      <fileset dir="${sourceLocation-AdapterDocRetrieveSecured}"/>
    </copy>
    <property name="targetLocation-AdapterDocRetrieveSecuredSchemas" location="${meta.inf}/wsdl/schemas/"/>
    <property name="sourceLocation-AdapterDocRetrieveSecuredSchemas" location="${meta.inf}/xml-resources/web-services/AdapterDocRetrieveSecured/schemas/"/>
    <copy todir="${targetLocation-AdapterDocRetrieveSecuredSchemas}">
      <fileset dir="${sourceLocation-AdapterDocRetrieveSecuredSchemas}" />
    </copy>
  </target>

  <target depends="init" if="netbeans.home" name="undeploy-clean" unless="from.commandline">
    <nbundeploy failOnError="false" startServer="false"/>
  </target>

  <target depends="init,compile-test,-pre-test-run,-do-test-run" if="have.tests" name="-post-test-run">
    <copy todir="${nhin.project.home}\UnitTestResults">
      <fileset dir="${build.test.results.dir}"/>
    </copy>
    <fail if="tests.failed" unless="ignore.failing.tests">Some tests failed; see details above.</fail>
  </target>    
</project>
