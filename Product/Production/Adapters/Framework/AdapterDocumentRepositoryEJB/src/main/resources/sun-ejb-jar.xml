<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 EJB 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-ejb-jar_3_0-0.dtd">
<sun-ejb-jar>
    <enterprise-beans>
        <ejb>
            <ejb-name>DocumentRegistryService</ejb-name>
            <webservice-endpoint>
                <port-component-name>DocumentRegistryService</port-component-name>
                <endpoint-address-uri>/DocumentRegistry/DocumentRegistry_Service</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
        <ejb>
            <ejb-name>DocumentRepositoryService</ejb-name>
            <webservice-endpoint>
                <port-component-name>DocumentRepositoryService</port-component-name>
                <endpoint-address-uri>/DocumentRepository/DocumentRepository_Service</endpoint-address-uri>
            </webservice-endpoint>   
        </ejb>
    </enterprise-beans>
</sun-ejb-jar>
