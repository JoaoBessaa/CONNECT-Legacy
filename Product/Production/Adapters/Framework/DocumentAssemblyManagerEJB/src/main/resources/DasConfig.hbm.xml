<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.hhs.fha.nhinc.assemblymanager.dao.model">
  <class name="DasConfig" table="das_config">
	<id name="attName" column="ATT_NAME" type="string" length="50">
		<generator class="native"></generator>
	</id>
  	<property name="attValue" column="ATT_VALUE" type="string" length="100" not-null="false" />
  	<property name="activeYn" column="ACTIVE_YN" type="string" length="1" not-null="true" />
    
  </class>
</hibernate-mapping>