            <xsd:import namespace="urn:gov:hhs:fha:nhinc:common:nhinccommonentity"
                  schemaLocation="../schemas/nhinc/common/NhincCommonEntity.xsd"/>
        </xsd:schema>
    </types>

    <message name="ProvideAndRegisterDocumentSet-bRequest_RequestMessage">
        <documentation>Provide and Register Document Set Request - Request</documentation>
        <part name="ProvideAndRegisterRequestRequest" element="nccommonentity:RespondingGateway_ProvideAndRegisterDocumentSetRequest"/>
    </message>
    <message name="ProvideAndRegisterDocumentSet-bRequest_ResponseMessage">
        <documentation>Provide And Register Document Set Request - Response</documentation>
        <part name="ProvideAndRegisterRequestResponse" element="xdr:Acknowledgement"/>
    </message>
    <portType name="EntityXDRRequest_PortType">

        <operation name="ProvideAndRegisterDocumentSet-bRequest">
            <input message="tns:ProvideAndRegisterDocumentSet-bRequest_RequestMessage"
        wsaw:Action="tns:ProvideAndRegisterDocumentSet-b"/>
            <output message="tns:ProvideAndRegisterDocumentSet-bRequest_ResponseMessage"
        wsaw:Action="tns:ProvideAndRegisterDocumentSet-bResponse"/>
        </operation>
    </portType>
    <binding name="EntityXDRRequest_Binding" type="tns:EntityXDRRequest_PortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>

        <operation name="ProvideAndRegisterDocumentSet-bRequest">
            <soap:operation soapAction="tns:ProvideAndRegisterDocumentSet-bRequest"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

    </binding>
    <service name="EntityXDRRequest_Service">
        <port name="EntityXDRRequest_Port" binding="tns:EntityXDRRequest_Binding">
            <soap:address location="http://localhost:${HttpDefaultPort}/EntityService/EntityXDRRequest"/>
        </port>
    </service>

</definitions>

